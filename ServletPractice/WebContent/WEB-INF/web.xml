<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ServletPractice</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>helloWorld</servlet-name>
    <servlet-class>com.mycompany.basic.HelloWorld</servlet-class>
    <init-param>
      <param-name>name</param-name>
      <param-value>paramesh</param-value>
    </init-param>
    <init-param>
      <param-name>company</param-name>
      <param-value>mycompany</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>helloWorld</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userPage</servlet-name>
    <servlet-class>com.mycompany.basic.InternalUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userPage</servlet-name>
    <url-pattern>/userPage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>passwordPage</servlet-name>
    <servlet-class>com.mycompany.basic.PasswordRedirect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>passwordPage</servlet-name>
    <url-pattern>/passwordPage</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>Global companyName</param-name>
    <param-value>Acs</param-value>
  </context-param>
</web-app>